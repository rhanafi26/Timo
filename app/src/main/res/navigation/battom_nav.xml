<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/battom"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.anggi.timo.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/profilFragment"
        android:name="com.anggi.timo.ProfilFragment"
        android:label="fragment_profil"
        tools:layout="@layout/fragment_profil" />

    <action
        android:id="@+id/action_profilFragment_to_dashboardFragment"
        app:destination="@id/dashboardFragment"

        app:popUpTo="@id/dashboardFragment"
        app:popUpToInclusive="true" />

    <fragment
        android:id="@+id/laporanFragment"
        android:name="com.anggi.timo.laporan.LaporanFragment"
        android:label="fragment_laporan"
        tools:layout="@layout/fragment_laporan" />
</navigation>